@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_ClientPage";
    ViewBag.Title = ViewBag.Title;
}
@model List<DemoAspNet.Models.Stat>
<h1 class="pb-3">Client </h1>
<hr />
<div class="row p-5">
    @foreach (DemoAspNet.Models.Stat stat in Model)
    {
        <div class="col">
            <partial name="_Stat" model="stat" />
        </div>
    }
</div>